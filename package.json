{
  "name": "prompt-protekt",
  "version": "1.0.0",
  "description": "Personal information detection using Hugging Face piiranha model",
  "main": "dist/huggingface.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/huggingface.js",
    "test": "npm run build && node dist/test.js",
    "dev": "ts-node huggingface.ts",
    "dev:test": "ts-node test.ts",
    "server": "ts-node server.ts",
    "server:build": "npm run build && node dist/server.js",
    "test:server": "ts-node test-server.ts",
    "worker:dev": "wrangler dev worker.ts",
    "worker:deploy": "wrangler deploy"
  },
  "dependencies": {
    "@huggingface/inference": "^4.13.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wrangler": "^3.0.0"
  }
}
